#include<stdio.h>
float area_of_triangle(int b, int h)
{
    return 0.5*b*h;
}
int main()
{
    int height, base;
    printf("Enter the height and base of a triangle: \n");
    scanf("%d %d", &height, &base );
    area_of_triangle(height,base);
    printf("Area of a triangle is %f\n", area_of_triangle(height,base));
}
